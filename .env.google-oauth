# Google OAuth Environment Variables Template
# Copy these to your .env.local file and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://brmh.in

# AWS Cognito Configuration
NEXT_PUBLIC_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
NEXT_PUBLIC_COGNITO_CLIENT_ID=your_app_client_id_here
NEXT_PUBLIC_COGNITO_DOMAIN=https://login.brmh.in
NEXT_PUBLIC_AWS_REGION=us-east-1

# OAuth Redirect URLs
NEXT_PUBLIC_REDIRECT_URI=https://auth.brmh.in/callback
NEXT_PUBLIC_LOGOUT_URI=https://auth.brmh.in/

# Development URLs (uncomment for local development)
# NEXT_PUBLIC_API_BASE_URL=http://localhost:5001
# NEXT_PUBLIC_REDIRECT_URI=http://localhost:3000/callback
# NEXT_PUBLIC_LOGOUT_URI=http://localhost:3000/

